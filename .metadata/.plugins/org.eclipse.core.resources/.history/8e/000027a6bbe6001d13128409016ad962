package com.jsp;

import org.springframework.stereotype.Component;

@Component(value="MP")
public class Person {
	
	
	int age;
	String name;
	
	
	
	public void setAge(int age) {
		this.age = age;
	}



	public void setName(String name) {
		this.name = name;
	}



	public void print()
	{
		System.out.println("you are from person");
	}

}
